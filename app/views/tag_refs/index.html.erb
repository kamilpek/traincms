<p id="notice"><%= notice %></p>

<h1>Tag Refs</h1>

<table>
  <thead>
    <tr>
      <th>Tag</th>
      <th>Article</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tag_refs.each do |tag_ref| %>
      <tr>
        <td><%= tag_ref.tag %></td>
        <td><%= tag_ref.article %></td>
        <td><%= link_to 'Show', tag_ref %></td>
        <td><%= link_to 'Edit', edit_tag_ref_path(tag_ref) %></td>
        <td><%= link_to 'Destroy', tag_ref, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tag Ref', new_tag_ref_path %>
